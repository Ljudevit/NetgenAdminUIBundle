{% extends 'NetgenMoreAdminUIBundle::pagelayout_legacy.html.twig' %}

{% set legacyPathArray = ezpublish.legacy.has( 'path' ) ? ezpublish.legacy.get( 'path' ) : [] %}
{% set pathArray = [] %}

{% for legacyPathItem in legacyPathArray %}
    {% set pathArray = pathArray|merge( [ {
        url: legacyPathItem.url ? path( 'ez_legacy', { 'module_uri': legacyPathItem.url } ) : false,
        text: legacyPathItem.text
    } ] ) %}
{% endfor %}

{% block content %}
    {{ module_result.content|raw }}
{% endblock %}
